<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\Shared.props" />

  <PropertyGroup>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <PropertyGroup>
    <Description>This Library provides great and usefull Extension Methods for caching</Description>
    <PackageTags>Cache</PackageTags>
  </PropertyGroup>


  <ItemGroup>
    <ProjectReference Include="..\Nextended.Core\Nextended.Core.csproj" />
    <PackageReference Include="Microsoft.Extensions.Caching.Abstractions" Version="$(ManagementVersion)" />
    <PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="$(ManagementVersion)" />
    <PackageReference Include="Microsoft.Extensions.Options" Version="$(MsVersion)" />
    <PackageReference Include="System.Runtime.Caching" Version="$(ManagementVersion)" />
  </ItemGroup>
  
</Project>
